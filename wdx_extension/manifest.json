{
  "manifest_version": 3,
  "name": "wdx Quellen",
  "version": "1.0.0",
  "description": "Sendet Webseitenquellen und Texte an wdx",
  "permissions": ["contextMenus", "activeTab", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "wdx Quellen"
  }
}